{
  "name": "noti.sh",
  "version": "1.0.0",
  "description": "Personal notifier",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf compiled dist",
    "lint": "eslint --ext .jsx,.js src/**",
    "prettier": "./scripts/prettier",
    "build": "babel src/client/ --out-dir compiled --source-maps --copy-files",
    "build:watch": "run-s 'build -- --watch'",
    "bundle:watch": "webpack --config ./config/webpack.config.js",
    "server:start": "node ./index.js",
    "server:debug": "nodemon --inspect-brk=${DEBUG_PORT:=9229} ./index.js",
    "start:watch": "nodemon ./index.js",
    "serve:static": "static-server --port=${STATIC_PORT:=3001} ./dist",
    "start": "npm-run-all -s build -p *:watch serve:static"
  },
  "keywords": [],
  "author": "<sickgrin@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.42",
    "@std/esm": "^0.26.0",
    "dotenv": "^5.0.1",
    "esm": "^3.0.14",
    "history": "^4.7.2",
    "koa": "^2.5.0",
    "koa-body": "^2.5.0",
    "koa-helmet": "^4.0.0",
    "koa-mount": "^3.0.0",
    "koa-route": "^3.2.0",
    "prop-types": "^15.6.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "universal-router": "^6.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.42",
    "@babel/core": "^7.0.0-beta.42",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.42",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.42",
    "@babel/preset-env": "^7.0.0-beta.42",
    "@babel/preset-es2017": "^7.0.0-beta.42",
    "@babel/preset-react": "^7.0.0-beta.42",
    "assets-webpack-plugin": "^3.5.1",
    "babel-loader": "^8.0.0-beta.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "nodemon": "^1.17.2",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.11.1",
    "redux-devtools-extension": "^2.13.2",
    "source-map-loader": "^0.2.3",
    "static-server": "^2.2.1",
    "webpack": "^4.4.1",
    "webpack-cli": "^2.0.13"
  },
  "private": null
}
